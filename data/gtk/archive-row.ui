<interface>
  <template class="ArchiveRow" parent="AdwActionRow">
    <child>
      <object class="GtkButton">
        <property name="icon-name">software-update-available-symbolic</property>
        <property name="visible">False</property>
        <property name="valign">center</property>
        <property name="tooltip-text" translatable="yes">Update Archive</property>
      </object>
    </child>

    <child>
      <object class="GtkButton">
        <property name="icon-name">user-trash-symbolic</property>
        <property name="valign">center</property>
        <property name="tooltip-text" translatable="yes">Remove Archive</property>
      </object>
    </child>

  </template>
</interface>
